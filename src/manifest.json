{
  "name": "cypress-data-cy-highlight",
  "description": "Highlights elements with data-cy attributes and displays their value. Elements can be exported to a file",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "cypress-data-cy.png"
  },
  "permissions": ["scripting", "activeTab", "downloads"],
  "web_accessible_resources": [
    {
      "resources": ["styles.css", "index.js", "html-panel.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
